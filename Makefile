#utilities	
	
$(BINDIR)/converter: $(SRCDIR)/converter.cpp
	$(GCC) -std=c++11 $(SRCDIR)/converter.cpp -o $(BINDIR)/converter

$(BINDIR)/generator: $(SRCDIR)/generator.cpp
	$(GCC) -std=c++11 $(SRCDIR)/generator.cpp -o $(BINDIR)/generator
	
$(BINDIR)/validator: $(SRCDIR)/validator.cpp
	$(GCC) $(SRCDIR)/validator.cpp -o $(BINDIR)/validator
	
$(BINDIR)/validboost: $(SRCDIR)/validboost.cpp
	$(GCC) $(SRCDIR)/validboost.cpp -o $(BINDIR)/validboost

$(BINDIR)/properer: $(SRCDIR)/properer.cpp
	$(GCC) $(SRCDIR)/properer.cpp -o $(BINDIR)/properer	
	
$(BINDIR)/frobboost: $(SRCDIR)/frobboost.cpp
	$(GCC) $(SRCDIR)/frobboost.cpp -o $(BINDIR)/frobboost	
	
$(BINDIR)/frobc: $(SRCDIR)/frobc.cpp
	$(GCC) $(SRCDIR)/frobc.cpp -o $(BINDIR)/frobc	
	
$(BINDIR)/outputter: $(SRCDIR)/outputter.cpp
	$(GCC) $(SRCDIR)/outputter.cpp -o $(BINDIR)/outputter	

.PHONY: clean all

clean:
	rm -f $(BINDIR)/*
	
cleancholc:
	rm -f $(BINDIR)/cholc*
	
cleancholboost:
	rm -f $(BINDIR)/cholboost*
	
cleancholmtc:
	rm -f $(BINDIR)/cholmtc*
	
cleancholmtboost:
	rm -f $(BINDIR)/cholmtboost*
